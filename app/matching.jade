extends ./layouts/_htmlbp5

block extra-css
  link(rel='stylesheet', href='material-design-lite/material.min.css')
  link(rel='stylesheet', href='stylesheets/material.min.css')
  link(rel='stylesheet', href='stylesheets/matching.css')

block main
  block iphone_menu
    include ./partials/_iphone_menu
  block main_menu
    include ./partials/_main_menu
  block section-matching-0
    include ./partials/_section-matching-0
  block section-matching-1
    include ./partials/_section-matching-1
  block section-matching-2
    include ./partials/_section-matching-2
  block section-matching-3
    include ./partials/_section-matching-3
  
  
  block outdated
    include ./partials/_outdated
  
  block js
    script(type='text/javascript', src='material-design-lite/material.min.js').
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js').
    script(type='text/javascript', src='material-design-lite/material.min.js').
    script(type='text/javascript', src='js/pickadate.js-3.5.6/lib/compressed/picker.js').
    script(type='text/javascript', src='js/pickadate.js-3.5.6/lib/compressed/picker.date.js').
    script(type='text/javascript', src='js/handlebars-v3.0.3.js').
    script(type='text/javascript', src='js/js-webshim/minified/polyfiller.js').
    script(src='//www.parsecdn.com/js/parse-1.5.0.min.js').
    script(type='text/javascript', src='js/underscore-min.js').
    script(type='text/javascript', src='js/moment-with-locales.min.js').
    script#student-template(type='text/x-handlebars-template').
      {{#each student}}
        <div class="row line-bottom">
          <div class="three columns career-profile career-profile-head">
              <div class="career-profile-head-pic">
                  <img src="http://placehold.it/80x80" alt="">
                  </div>
                  <div class="career-profile-head-name">
                      <p>Alberto Camogli</p>
                  </div>
                  <div class="career-profile-head-button">
                      <button class="mdl-button mdl-button-profile mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--accent" data-upgraded=",MaterialButton,MaterialRipple">Contatta
                          <span class="mdl-button__ripple-container">
                              <span class="mdl-ripple"></span>
                          </span>
                      </button>
                  </div>
              </div>
              <div class="seven columns career-profile career-profile-bio">
                  <div class="career-profile-bio-title">
                      <p>Bio</p>
                  </div>
                  <div class="career-profile-bio-main">
                      <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum.</p>
                  </div>
              </div>
              <div class="six columns career-profile career-profile-skills">
                  <div class="career-profile-bio-title">
                      <p>Skills</p>
                  </div>
                  <ul>
                      <li>UX Design</li>
                      <li>Graphic Design</li>
                      <li>HTML</li>
                      <li>Gatto</li>
                      <li>Cane</li>
                  </ul>
            </div>
        </div>
      {{/each}}
    script(src='/js/matching.js').
    
